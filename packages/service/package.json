{
  "name": "@bc-nx-test-org/service",
  "version": "1.1.0",
  "description": "",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barnabycollins/monorepo-playground.git",
    "directory": "packages/service"
  },
  "scripts": {
    "build:cjs": "npx tsc --module commonjs --outDir dist/cjs",
    "build:esm": "npx tsc --module nodenext --outDir dist/esm",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "npx vitest run"
  },
  "dependencies": {
    "@bc-nx-test-org/logger": "^1.1.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "module": "./dist/esm/index.js"
}
